# Frontend Environment Configuration
VITE_BACKEND_URL=http://localhost:8000
VITE_WS_URL=ws://localhost:8000
VITE_APP_NAME=UMMAH AI Platform
VITE_APP_VERSION=2.0.0

# API Endpoints
VITE_API_BASE_URL=http://localhost:8000/api
VITE_REALTIME_API_URL=http://localhost:8000/api/v1/realtime
VITE_TRADING_API_URL=http://localhost:8000/api/trading
VITE_GPT_API_URL=http://localhost:8000/api/v1/gpt-agents

# WebSocket Configuration
VITE_WS_RECONNECT_INTERVAL=5000
VITE_WS_MAX_RETRIES=10

# Feature Flags
VITE_ENABLE_VOICE_COMMANDS=true
VITE_ENABLE_3D_VISUALIZATION=true
VITE_ENABLE_XR_MODE=true
VITE_ENABLE_STEALTH_MODE=true

# Theme Configuration
VITE_DEFAULT_THEME=dark
VITE_ENABLE_THEME_SWITCHING=true
